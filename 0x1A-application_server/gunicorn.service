[Unit]
Description=Gunicorn process for Airbnb application
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/AirBnB_clone_v4
ExecStart=/etc/systemd/system/gunicorn.service --workers 3 --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app >> /tmp/airbnb-access.log 2> /tmp/airbnb-error.log
Restart=always

[Install]
WantedBy=multi-user.target
